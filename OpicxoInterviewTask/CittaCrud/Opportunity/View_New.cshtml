@{
    ViewBag.Title = "View Opportunity";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!-- Header :: Start -->
<div class="header-area">
    <div class="card">
        <div class="card-header bb-0">
            <div class="card-header-title">
                <h4>View Opportunity</h4>
                <div class="header-breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="@Url.Action(" Dashboard","Home")"><i data-feather="home"></i></a></li>
                        <li class="breadcrumb-item"><a href="@Url.Action(" Dashboard","Home")">Dashboard</a></li>
                        <li class="breadcrumb-item active">View Opportunity</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Header :: End -->
<!-- Main Area :: Start -->
<section class="main-area-section">
    <div class="row">
        <div class="col-xl-12 col-lg-12">
            <ul class="nav head-nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item">
                    <button type="button" class="nav-link active" id="tab_1" data-target="#tab-1" data-toggle="tab" aria-selected="true">Lead Details</button>
                </li>
                <li class="nav-item">
                    <button type="button" class="nav-link" id="tab_2" data-target="#tab-2" data-toggle="tab" aria-selected="false">Contact Details</button>
                </li>
                <li class="nav-item">
                    <button type="button" class="nav-link" id="tab_3" data-target="#tab-3" data-toggle="tab" aria-selected="false">Location Preference</button>
                </li>
                <li class="nav-item">
                    <button type="button" class="nav-link" id="tab_4" data-target="#tab-4" data-toggle="tab" aria-selected="false">Feature Preference</button>
                </li>
                <li class="nav-item">
                    <button type="button" class="nav-link" id="tab_5" data-target="#tab-5" data-toggle="tab" aria-selected="false">Key Attributes</button>
                </li>
                <li class="nav-item" style="margin-left: auto">
                    @if ((Model.PI >= 0) && (Model.PS > 0))
                    {
                        var PIval = Model.PI;
                        var PSval = Model.PS;
                        var ViewType = Model.ViewType;
                        var EditId = Helper.AppCommon.EncryptString(Convert.ToString(Model.Id));
                        <a href="@Url.Action("List","Opportunity", new { Area = "Transactions",PI = PIval,PS = PSval,ViewType = ViewType,EditId = EditId})" class="btn" style="background-color: #fdb813; font-size: 17px; ">Back</a>
                    }
                    else
                    {
                        <a href="@Url.Action("List","Opportunity", new { Area = "Transactions"})" class="btn" style="background-color: #fdb813; font-size: 17px; ">Back</a>
                    }
                    
                </li>
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="tab-1">
                    <div class="card card-flush">
                        <div class="card-header">
                            <div class="card-title">
                                <h4 class="card-box-title">Lead Details</h4>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="card-dashed-box">
                                <div class="card-dashed-info-area">
                                    <div class="row">
                                        <div class="col-xl-4 col-lg-12">
                                            <div class="form-group-box">
                                                <div class="form-group-label">Lead ID</div>
                                                <div class="form-group-value">@Model.LeadNo</div>
                                            </div>
                                            <div class="form-group-box">
                                                <div class="form-group-label">Lead Date</div>
                                                <div class="form-group-value">@Model.LeadDate</div>
                                            </div>
                                            <div class="form-group-box">
                                                <div class="form-group-label">Lead Business Type</div>
                                                @if (Model.LeadBusinessType == "")
                                                {
                                                    <div class="form-group-value"><i>Not Defined</i></div>
                                                }
                                                else
                                                {
                                                    <div class="form-group-value">@Model.LeadBusinessType</div>
                                                }
                                            </div>
                                            <div class="form-group-box">
                                                <div class="form-group-label">Property Category</div>
                                                <div class="form-group-value">
                                                    @if (Model.PropertyCategoryIds != null)
                                                    {
                                                        if (Model.PropertyCategoryIds.Count > 0)
                                                        {
                                                            foreach (KeyValuePair<int?, string> entry in Model.PropertyCategoryIds)
                                                            {
                                                                if (@entry.Value.ToString().ToLower().Contains("residential"))
                                                                {
                                                                    <span class="tb-location-name icon-primary"><i class="fas fa-home"></i>@entry.Value</span>
                                                                }
                                                                else if (@entry.Value.ToString().ToLower().Contains("commercial"))
                                                                {
                                                                    <span class="tb-location-name icon-primary"><i class="fas fa-building"></i>@entry.Value</span>
                                                                }
                                                                else if (@entry.Value.ToString().ToLower().Contains("industrial"))
                                                                {
                                                                    <span class="tb-location-name icon-primary"><i class="fas fa-industry"></i>@entry.Value</span>
                                                                }
                                                                else if (@entry.Value.ToString().ToLower().Contains("land"))
                                                                {
                                                                    <span class="tb-location-name icon-primary"><i class="fas fa-sign"></i>@entry.Value</span>
                                                                }
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <span class="form-group-value"><i>Not Defined</i></span>
                                                        }

                                                    }
                                                    else
                                                    {
                                                        <span class="tb-location-name icon-primary"><i>Not Defined</i></span>
                                                    }
                                                </div>
                                            </div>
                                        </div>
