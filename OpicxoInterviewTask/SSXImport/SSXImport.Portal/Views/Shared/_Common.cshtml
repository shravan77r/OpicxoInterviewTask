<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Smart Search X Career Center</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="shortcut icon" href="~/images/favicon.png">
    <link rel="stylesheet" href="~/css/bootstrap.css" />
    <link rel="stylesheet" href="~/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/css/animate.css" />
    <link rel="stylesheet" href="~/css/plugins/select2/select2.min.css" />
    <link rel="stylesheet" href="~/css/plugins/select2/select2-bootstrap4.min.css" />
    <link rel="stylesheet" href="~/css/plugins/daterangepicker/daterangepicker.css" />
    <link rel="stylesheet" href="~/css/plugins/datepicker/datepicker3.css" />
    <link rel="stylesheet" href="~/css/plugins/dataTables/datatables.min.css">
    <link href="~/css/plugins/timepicker/bootstrap-timepicker.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/custom.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/theme-dark.css" asp-append-version="true" />

    <link href="~/css/devextreme/Charset.css" rel="stylesheet" />
    @*<link href="~/css/devextreme/bootstrap.css" rel="stylesheet" />*@
    <link href="~/css/devextreme/dx.common.css" rel="stylesheet" />
    <link href="~/css/devextreme/dx.light.css" rel="stylesheet" />

    <script src="~/js/jquery-3.1.1.min.js"></script>
   
    <script src="https://unpkg.com/devextreme-quill/dist/dx-quill.min.js"></script>
    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}
</head>
<body>

    <div id="wrapper" class="@Html.PageClass()">


        <partial name="_Navigation" />


        <div id="page-wrapper" class="gray-bg @ViewBag.SpecialClass">

            <partial name="_TopNavbar" />

            @RenderBody()

            <partial name="_Footer" />

        </div>


    </div>


    @*<script src="~/js/jquery-3.1.1.min.js"></script>*@
    @*<script type="text/javascript" src="https://code.jquery.com/jquery-3.5.1.min.js"></script>*@

    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.js"></script>
    <script src="~/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/js/plugins/pace/pace.min.js"></script>
    <script src="~/js/plugins/select2/select2.full.min.js"></script>
    <script src="~/js/plugins/datepicker/bootstrap-datepicker.js"></script>
    <script src="~/js/plugins/daterangepicker/daterangepicker.js"></script>
    <script src="~/js/moment.min.js"></script>
    <script src="~/js/plugins/daterangepicker/daterangepicker.min.js"></script>
    <script src="~/js/plugins/fullcalendar/moment.min.js"></script>
    <script src="~/js/plugins/dataTables/datatables.min.js"></script>
    <script src="~/js/plugins/dataTables/dataTables.bootstrap4.min.js"></script>
    <script src="~/js/plugins/timepicker/bootstrap-timepicker.min.js"></script>
    <script src="~/js/script.js" asp-append-version="true"></script>

    


    @RenderSection("scripts", required: false)
</body>
</html>
