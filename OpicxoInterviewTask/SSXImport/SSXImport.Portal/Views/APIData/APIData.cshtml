@{
    Layout = "~/Views/Shared/_Common.cshtml";
    ViewData["Title"] = "API Template";
}
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title  pt-2">
                    <a class="btn btn-white" href="@Url.Action("APIDataForm","APIData")"> <i class="fa fa-plus"></i> Add New API Template</a>
                </div>
                <div class="ibox-content">
                    <div class="" id="divAlertMessage">
                    </div>
                    <div class="form-group row">
                        <div class="col-md-4">
                            <input type="text" class="form-control" value="" placeholder="Search" id="search">
                        </div>
                        <div class="col-md-4 d-none">
                            <button class="btn btn-primary" type="button" id="btnSearch"><i class="fa fa-search"></i> Search</button>
                        </div>
                    </div>
                    <div class="table-responsive">
                        <table class="table  table-bordered dataTables" width="100%" id="tblAPITemplate">
                            <thead>
                                <tr>
                                    <th width="2.5%">#</th>
                                    <th>API Template Name</th>
                                    <th>Endpoint</th>
                                    <th>Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    <environment include="Development">
        <script src="~/js/APIData/ApidataList.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/APIData/ApidataList.min.js"></script>
    </environment>
}