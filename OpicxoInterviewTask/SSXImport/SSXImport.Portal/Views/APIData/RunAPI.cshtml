@model SSXImport.Portal.Models.APIExecutionResponse
@{
    Layout = "~/Views/Shared/_Common.cshtml";
    ViewData["Title"] = "API Response";
}
<div class="loading-backdrop customloader" style="display:none">
    <div class="sk-spinner sk-spinner-double-bounce">
        <div class="sk-double-bounce1"></div>
        <div class="sk-double-bounce2"></div>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-content">
                    <div class="" id="divAlertMessage">
                    </div>
                    <form id="form" action="#" class="wizard-big">
                        <h1>API Response</h1>
                        <fieldset>

                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="APName">APITemplate<span class="text-danger">*</span> </label>
                                        @Html.TextBoxFor(o => o.Name, new { @class = "form-control required", @maxlength = "50", @autocomplete = "off", @placeholder = "Enter API Name", @disabled = "disabled" })
                                        @Html.ValidationMessageFor(o => o.Name, "", new { @class = "error" })
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="APIEndPoint">API End Point<span class="text-danger">*</span> </label>
                                        @Html.TextBoxFor(o => o.APIEndPoint, new { @class = "form-control required", @maxlength = "50", @autocomplete = "off", @placeholder = "Enter API End Point", @disabled = "disabled" })
                                        @Html.ValidationMessageFor(o => o.APIEndPoint, "", new { @class = "error" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <textarea class="form-control" id="response" rows="20">
                                        </textarea>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

@section Styles {

}
@section Scripts {
    <script>
        $(function () {
            // using JSON.stringify pretty print capability:
            var str = JSON.stringify('@Model.APIResultData', undefined, 4);

            // display pretty printed object in text area:
            document.getElementById('response').innerHTML = str;
        });
    </script>
}